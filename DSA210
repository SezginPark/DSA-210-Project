        "plt.ylabel('Percent Increase (%)')\n",
        "plt.ylim(0, means['Pct Increase'].max() * 1.2)\n",
        "\n",
        "# annotate values\n",
        "for bar in bars:\n",
        "    height = bar.get_height()\n",
        "    plt.text(\n",
        "        bar.get_x() + bar.get_width()/2,\n",
